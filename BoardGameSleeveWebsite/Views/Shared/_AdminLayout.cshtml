@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Board Game Sleeves Admin</title>
    <link href="/css/style.css" rel="stylesheet" />
    <link href="~/css/administration.css" rel="stylesheet" />
    @RenderSection("css", false)
    <script src="https://code.jquery.com/jquery-3.1.1.min.js"
            integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
            crossorigin="anonymous"></script>


    <script>
        $(document).ready(function () {
            windowsize = 0;
            windowsize = $(document).height() - 40;
            $(".administrationsNavn").height(windowsize);
        });
    </script>
</head>
<body>
    <img src="~/img/progress.gif" id="progress" />
    <div style="width: 100%;overflow: hidden;">
        <div class="row">
            <nav class="col-md-3 administrationsNavn">
                <h1>
                    <a href="/admin/product"><img src="~/img/logo.png" width="50%" height="50%" class="admin-logo" /></a>
                </h1>

                <ul>
                    <li>
                        <a href="/admin/product">Product</a>
                    </li>
                    <li>
                        <a href="/admin/size">Size</a>
                    </li>
                    <li>
                        <a href="/admin/Game">Game</a>
                    </li>
                    <li>
                        <a href="/" target="_blank">Go to Frontpage</a>
                    </li>
                </ul>
            </nav>
            <div class="col-md-9">
                @RenderBody()
            </div>
        </div>
    </div>
    <script src="~/scripts/script.js"></script>
    @RenderSection("script", false)
</body>
</html>
